class Solution {
public:
    vector<vector<int>> transpose(vector<vector<int>>& A) {
        if(A.size() == 0) return {{}};
        int m = A.size(), n = A[0].size();
        vector<vector<int>> res(n, vector<int>(m, 0));
        
        for(int i = 0; i < m; i++){
            for(int j = 0; j < n; j++){
                res[j][i] =  A[i][j];
            }
        }
        return res;
    }
};
